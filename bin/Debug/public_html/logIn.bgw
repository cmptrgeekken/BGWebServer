<html>
<head>
<title>Log In</title>
<link rel="stylesheet" href="BGStyle.css" />
</head>
<body onLoad="document.forms[0].elements['username'].focus()">
%INCLUDEFILE<file>menu.bgw</file>%/INCLUDEFILE
<form method="post" action="index.bgw">
<input type="hidden" name="action" value="logIn">
<table align="center">
   <tr>
      <td>Nickname: </td><td><input type="text" name="username"></td>
   </tr>
   <tr>
      <td>Password: </td><td><input type="password" name="password"></td>
   </tr>
   <tr>
      <td colspan="2" align="center"><input type="submit" value="Log In"></td>
   </tr>
</table>
</form>
</body>
</html>